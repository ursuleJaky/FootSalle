<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
   
<hibernate-mapping>
<class name="beans.Personne" table="PERSONNES">
	<id name="id" column="id_personne">
	<generator class="increment"></generator>
	</id>
	
	
	<property name="nom" column="nom_personne"></property>
	<property name="prenom" column="prenom_personne"></property>
	<property name="tel" column="tel_personne"></property>
	<property name="email" column="email_personne"></property>
	
	
	</class>
</hibernate-mapping>